

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:pass123@localhost:5432/authentication"
}

model User {
  id    String  @unique @default(uuid()) 
  email String  @unique
  password String 
  createdAt DateTime @default(now())
  updateAt DateTime @updatedAt()
  otp      String?
}
